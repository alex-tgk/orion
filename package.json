{
  "name": "orion",
  "version": "1.0.0",
  "description": "ORION - A revolutionary microservices platform with CLI-based AI integration",
  "main": "index.js",
  "scripts": {
    "dev": "nx run-many --target=serve --all --parallel",
    "dev:service": "nx serve",
    "dev:affected": "nx affected:serve",
    "test": "nx affected:test --parallel",
    "test:all": "nx run-many --target=test --all --parallel",
    "test:e2e": "nx e2e",
    "test:coverage": "nx affected:test --coverage",
    "test:ci": "nx run-many --target=test --all --coverage --maxWorkers=2",
    "test:integration": "nx run-many --target=test:integration --all",
    "lint": "nx affected:lint",
    "type-check": "tsc --noEmit",
    "lint:fix": "nx affected:lint --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "build": "nx affected:build --parallel",
    "build:all": "nx run-many --target=build --all --parallel",
    "build:prod": "nx affected:build --prod",
    "build:auth": "nx build auth",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "spec:generate": "nx run dev-tools:generate-spec",
    "spec:validate": "nx run dev-tools:validate-specs",
    "spec:coverage": "nx run dev-tools:spec-coverage",
    "reflect": "nx run dev-tools:reflect",
    "ai:assist": "nx run mcp-server:assist",
    "ai:suggest": "nx run dev-tools:suggest-next",
    "health": "nx run dev-tools:health-check",
    "metrics": "nx run dev-tools:show-metrics",
    "diagnose": "nx run dev-tools:diagnose",
    "docs:generate": "npm run docs:typedoc && npm run docs:compodoc",
    "docs:typedoc": "typedoc --options typedoc.json",
    "docs:compodoc": "compodoc -p packages/auth/tsconfig.json -d docs/generated/auth --theme material",
    "docs:serve": "npx serve docs -p 8000",
    "docs:watch": "typedoc --options typedoc.json --watch",
    "clean": "nx reset && rm -rf dist",
    "deps:check": "nx dep-graph",
    "migrate": "nx migrate latest",
    "prepare": "husky"
  },
  "keywords": [
    "microservices",
    "nestjs",
    "ai",
    "cli-integration",
    "self-improving",
    "observable",
    "nx-monorepo"
  ],
  "author": "ORION Team",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "^21.6.5",
    "@nx/jest": "^21.6.5",
    "@nx/js": "^21.6.5",
    "@nx/nest": "^21.6.5",
    "@nx/node": "^21.6.5",
    "@nx/web": "21.6.5",
    "@nx/webpack": "21.6.5",
    "@nx/workspace": "^21.6.5",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-query-devtools": "^5.62.16",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.8.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@compodoc/compodoc": "^1.1.25",
    "typedoc": "^0.26.0",
    "typedoc-plugin-markdown": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.4",
    "nx": "^21.6.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "^5.9.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.17.1",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fp-ts": "^2.16.11",
    "ioredis": "^5.8.1",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0"
  }
}
